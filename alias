# tmux
alias tmns="tmux new -t"
alias tmas="tmux a -t"
alias tmks="tmuz kill-session -t"

# git
alias gl='git log --all --graph --decorate'
alias gs='git status'
alias gaa='git add .'
alias x='clear'

# sync
alias ns='bash $HOME/Code/dotfiles/sync_notes.sh'
alias expsync='bash $HOME/Code/expense_tracker/sync.sh'